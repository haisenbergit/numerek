{
  "name": "grupa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"NEXT,CONVEX\" --prefix-colors \"yellow.bold,blue.bold\" --prefix \"[{time}] [{name}]\" --timestamp-format \"HH:mm:ss\" --kill-others-on-fail \"npm run dev:next\" \"npm run dev:convex\"",
    "dev:next": "next dev --turbopack",
    "dev:convex": "npx convex dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "type-check": "tsc --noEmit",
    "validate-all": "npm run lint && npm run type-check && npm run format:check && npm run build"
  },
  "dependencies": {
    "@auth/core": "^0.37.0",
    "@convex-dev/auth": "^0.0.90",
    "@oslojs/crypto": "^1.0.1",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "resend": "^6.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
