{
  "name": "grupa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"NEXT,CONVEX\" --prefix-colors \"yellow.bold,blue.bold\" --prefix \"[{time}] [{name}]\" --timestamp-format \"HH:mm:ss\" --kill-others-on-fail \"npm run dev:next\" \"npm run dev:convex\"",
    "dev:next": "next dev --turbopack",
    "dev:convex": "npx convex dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "validate-all": "npm run lint && npm run type-check && npm run format:check && npm run test:run && npm run build"
  },
  "dependencies": {
    "@auth/core": "^0.37.0",
    "@convex-dev/auth": "^0.0.90",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@oslojs/crypto": "^1.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.29.0",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.16.1",
    "jotai": "^2.15.1",
    "lucide-react": "^0.546.0",
    "next": "^15.5.9",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.6",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^3.0.6",
    "react-use": "^17.6.0",
    "react-verification-input": "^4.2.2",
    "resend": "^6.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "convex-test": "^0.0.38",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "fast-glob": "^3.3.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^4.0.6"
  }
}
